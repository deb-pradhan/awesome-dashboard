# Cursor API Key (required for background agent automation)
# Get from: https://cursor.com/settings/api
CURSOR_API_KEY=cur_xxxx

# Anthropic API Key (for local pipeline testing)
ANTHROPIC_API_KEY=sk-ant-api03-xxxx

# Optional: OpenAI for AI insights in the dashboard
OPENAI_API_KEY=sk-xxxx

# Optional: Redis for caching (uses memory cache if not set)
REDIS_URL=redis://localhost:6379

# Optional: Postgres for persistence
DATABASE_URL=postgresql://user:pass@localhost:5432/dashboard
